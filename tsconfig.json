{
  "compilerOptions": {
    "target": "ES2020",
    "module": "NodeNext",
    "alwaysStrict": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "noEmit": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "pretty": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
  }
}